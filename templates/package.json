{
  "private": true,
  "name": "{SLUSH{=slug}}",
  "version": "{SLUSH{=version}}",
  "description": "{SLUSH{=description}}",
  "author": "{SLUSH{=author}}",
  "generator": "slush-gh-pages v{SLUSH{=generatorVersion}}",
  "license": "UNLICENSED",
  "homepage": "https://github.com/{SLUSH{=github}}",
  "repository": {
    "type": "git",
    "url": "git://github.com/{SLUSH{=github}}.git"
  },
  "bugs": {
    "url": "https://github.com/{SLUSH{=github}}/issues"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "scripts": {
    {SLUSH{ if (framework === 'bootstrap3') { }}"install-bootstrap-glypicons": "cp -R node_modules/bootstrap-sass/assets/fonts/ _assets/fonts",{SLUSH{ } }}
    "clean": "rm -rf {assets/stylesheets/*,assets/javascripts/*,assets/images/*,assets/fonts/*}",
    "postcss": "NODE_ENV=production postcss --config postcss.config.js --replace assets/stylesheets/styles.css",
    "postcss:dev": "postcss --config postcss.config.js --map --replace assets/stylesheets/styles.css",
    "scss": "node-sass --importer node_modules/node-sass-import _assets/stylesheets/main.scss assets/stylesheets/styles.css",
    "scss:dev": "node-sass --importer node_modules/node-sass-import --source-map true _assets/stylesheets/main.scss assets/stylesheets/styles.css",
    "lint:js": "eslint _assets/javascripts",
    "lint:css": "stylelint _assets/stylesheets/**/*.scss --syntax scss",
    "lint": "npm-run-all -p lint:*",
    "imagemin": "imagemin _assets/images/* -o assets/images",
    "icons": "svgo -f _assets/images/icons && mkdir -p assets/images && svg-sprite-generate -d _assets/images/icons -o assets/images/icons.svg",
    "serve": "node -e 'setTimeout(function() {}, 1500)' && browser-sync start --server _site --port 8888 --files _site --reload-debounce",
    "build:jekyll": "JEKYLL_GITHUB_TOKEN=$([ -f .githubtoken ] && cat .githubtoken) JEKYLL_ENV=production bundle exec jekyll build --config _config.yml",
    "build:fonts": "mkdir -p assets/fonts && cp -R _assets/fonts/ assets/fonts",
    "build:css": "npm run scss && npm run postcss",
    "build:css:dev": "npm run scss:dev && npm run postcss:dev",
    "build:js": "NODE_ENV=production rollup --config --input _assets/javascripts/main.js --output assets/javascripts/scripts.js",
    "build:js:dev": "rollup --config --sourcemap --input _assets/javascripts/main.js --output assets/javascripts/scripts.js",
    "build:images": "npm run imagemin && npm run icons",
    "build": "npm run clean &&  npm-run-all -p build:*",
    "watch:jekyll": "JEKYLL_GITHUB_TOKEN=$([ -f .githubtoken ] && cat .githubtoken) JEKYLL_ENV=development bundle exec jekyll build --watch --incremental --config _config.yml,_config-development.yml || true",
    "watch:css": "chokidar '_assets/stylesheets/**/*' -d -c 'npm run build:css:dev'",
    "watch:js": "chokidar '_assets/javascripts/**/*' -d -c 'npm run build:js:dev'",
    "watch:images": "chokidar '_assets/images/**/*' -d -c 'npm run build:images'",
    "watch:fonts": "chokidar '_assets/fonts/**/*' -d -c 'npm run build:fonts'",
    "watch": "npm run clean && npm run build:images && npm run build:fonts && npm run build:js:dev && npm run build:css:dev && npm-run-all --continue-on-error --parallel watch:* serve",
    "postinstall": "bundle install && chmod +x __tests__/test.sh{SLUSH{ if (framework === 'bootstrap3') { }} && npm run install-bootstrap-glypicons {SLUSH{ } }}&& npm test",
    "preversion": "npm test",
    "version": "git add -A assets",
    "postversion": "git push && git push --tags",
    "pretest": "npm run lint",
    "test": "npm-run-all --parallel build:fonts build:css build:js build:images && JEKYLL_GITHUB_TOKEN=$([ -f .githubtoken ] && cat .githubtoken) JEKYLL_ENV=production __tests__/test.sh",
    "deploy": "npm version minor",
    "start": "npm run watch"
  },
  "devDependencies": {
    {SLUSH{ if (framework === 'concise') { }}
    "postcss-custom-media": "^5.0.1",
    "postcss-lh": "^1.1.3",
    "postcss-media-minmax": "^2.1.2",
    "postcss-pr": "^1.0.1",
    {SLUSH{ } }}
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.14.0",
    "chokidar-cli": "^1.2.0",
    "cssnano": "^3.7.4",
    "eslint": "^3.2.2",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "imagemin-cli": "^3.0.0",
    "node-sass": "^3.7.0",
    "node-sass-import": "^1.0.2",
    "npm-run-all": "^2.1.1",
    "postcss-cli": "^2.5.2",
    "postcss-urlrev": "^1.3.0",
    "rollup": "^0.34.3",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "stylelint": "^7.1.0",
    "stylelint-config-standard": "^12.0.0",
    "svg-sprite-generator": "0.0.7",
    "svgo": "^0.6.6"
  },
  "dependencies": {
    {SLUSH{ if (framework === 'concise') { }}
    "concise-utils": "git://github.com/concisecss/concise-utils.git#master",
    "concise.css": "git://github.com/concisecss/concise.css.git#master",
    {SLUSH{ } }}
    {SLUSH{ if (framework === 'bootstrap4') { }}
    "jquery": "^2.2.4",
    "bootstrap": "^4.0.0-alpha.3",
    "tether": "^1.3.4",
    {SLUSH{ } }}
    {SLUSH{ if (framework === 'bootstrap3') { }}
    "jquery": "^2.2.4",
    "bootstrap-sass": "^3.3.7",
    {SLUSH{ if (bootswatch !== 'none') { }}
    "bootswatch": "^3.3.7",
    {SLUSH{ } }}
    {SLUSH{ } }}
    "svg4everybody": "^2.1.0"
  }
}
