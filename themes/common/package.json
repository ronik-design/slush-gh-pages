{
  "private": true,
  "name": "{SLUSH{=slug}}",
  "version": "{SLUSH{=version}}",
  "description": "{SLUSH{=description}}",
  "author": {
    "name": "{SLUSH{=author}}",
    "email": "{SLUSH{=email}}"
  },
  "generator": "slush-gh-pages v{SLUSH{=generatorVersion}}",
  "license": "UNLICENSED",
  "homepage": "https://github.com/{SLUSH{=github}}",
  "repository": {
    "type": "git",
    "url": "git://github.com/{SLUSH{=github}}.git"
  },
  "bugs": {
    "url": "https://github.com/{SLUSH{=github}}/issues"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "scripts": {
    "clean": "rm -rf {assets/stylesheets/*,assets/javascripts/*,assets/images/*,assets/fonts/*}",
    "postcss": "NODE_ENV=production postcss --config postcss.config.js --replace assets/stylesheets/styles.css",
    "postcss:dev": "postcss --config postcss.config.js --map --replace assets/stylesheets/styles.css",
    "scss": "node-sass --importer node_modules/node-sass-import _assets/stylesheets/main.scss assets/stylesheets/styles.css",
    "scss:dev": "node-sass --importer node_modules/node-sass-import --source-map true _assets/stylesheets/main.scss assets/stylesheets/styles.css",
    "lint:js": "eslint _assets/javascripts",
    "lint:css": "stylelint _assets/stylesheets/**/*.scss --syntax scss",
    "lint": "npm-run-all -p lint:*",
    "imagemin": "imagemin _assets/images/* -o assets/images",
    "icons": "svgo -f _assets/images/icons && mkdir -p assets/images && svg-sprite-generate -d _assets/images/icons -o assets/images/icons.svg",
    "serve": "node -e 'setTimeout(function() {}, 2000)' && browser-sync start --server _site --port 5000 --files '_site/**/*, !_site/sitemap.xml, !_site/feed.xml' --reload-debounce",
    "jekyll:build": "JEKYLL_GITHUB_TOKEN=$([ -f .githubtoken ] && cat .githubtoken) JEKYLL_ENV=production bundle exec jekyll build --safe --config _config.yml",
    "jekyll:watch": "JEKYLL_GITHUB_TOKEN=$([ -f .githubtoken ] && cat .githubtoken) JEKYLL_ENV=development bundle exec jekyll build --safe --watch --incremental --config _config.yml,_config-development.yml || true",
    "build:stylesheets": "npm run scss && npm run postcss",
    "build:stylesheets:dev": "npm run scss:dev && npm run postcss:dev",
    "build:images": "npm run imagemin && npm run icons",
    "build:fonts": "mkdirp assets/fonts && cp -R _assets/fonts/ assets/fonts",
    "build:javascripts": "NODE_ENV=production webpack -p _assets/javascripts/main.js assets/javascripts/scripts.js",
    "build:javascripts:dev": "webpack -d _assets/javascripts/main.js assets/javascripts/scripts.js",
    "build": "npm-run-all --serial clean build:images build:fonts build:stylesheets build:javascripts jekyll:build",
    "watch:css": "chokidar '_assets/stylesheets/**/*' -d -c 'npm run build:stylesheets:dev'",
    "watch:js": "webpack --watch _assets/javascripts/main.js assets/javascripts/scripts.js",
    "watch:images": "chokidar '_assets/images/**/*' -d -c 'npm run build:images'",
    "watch:fonts": "chokidar '_assets/fonts/**/*' -d -c 'npm run build:fonts'",
    "watch": "npm-run-all --serial clean build:images build:fonts build:javascripts:dev build:stylesheets:dev && npm-run-all --continue-on-error --parallel watch:* jekyll:watch serve",
    "postinstall": "bundle install && chmod +x __tests__/test.sh && npm test",
    "preversion": "npm test",
    "version": "git add -A assets",
    "postversion": "git push && git push --tags",
    "pretest": "npm run lint",
    "test": "npm-run-all --serial clean build:images build:fonts build:stylesheets build:javascripts && JEKYLL_GITHUB_TOKEN=$([ -f .githubtoken ] && cat .githubtoken) JEKYLL_ENV=production __tests__/test.sh",
    "deploy": "npm version minor",
    "start": "npm run watch"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.15.0",
    "chokidar-cli": "^1.2.0",
    "cssnano": "^3.7.4",
    "eslint": "^3.4.0",
    "eslint-config-xo-space": "^0.14.0",
    "eslint-plugin-babel": "^3.3.0",
    "html-loader": "^0.4.3",
    "imagemin-cli": "^3.0.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.9.3",
    "node-sass-import": "^1.0.5",
    "npm-run-all": "^3.1.0",
    "postcss-cli": "^2.6.0",
    "postcss-urlrev": "^1.3.0",
    "stylelint": "^7.2.0",
    "stylelint-config-standard": "^13.0.0",
    "svg-sprite-generator": "0.0.7",
    "svgo": "^0.7.0",
    "webpack": "^2.1.0-beta.22"
  },
  "dependencies": {
    "core-js": "^2.4.1",
    "svg4everybody": "^2.1.0",
    "raf-ready": "^1.0.0"
  }
}
